{
    "routes": [
        {
            "src": "/scripts/(.*)",
            "dest": "/scripts/$1"
        },
        {
            "src": "/style/(.*)",
            "dest": "/style/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "/app.py"
        },
        {
            "src": "/diagonalize",
            "dest": "/diagonalize.html"
        },
        {
            "src": "/aboutus",
            "dest": "/aboutus.html"
        },
        {
            "src": "/",
            "dest": "/home.html"
        }
    ]
}

