{
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/home.html"
        },
        {
            "src": "/aboutus",
            "dest": "/aboutus.html"
        },
        {
            "src": "/diagonalize",
            "dest": "/diagonalize.html"
        },
        {
            "src": "/scripts/(.*)",
            "dest": "/scripts/$1"
        },
        {
            "src": "/style/(.*)",
            "dest": "/style/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "app.py"
        }
    ]
}
