{
    "builds": [
        {
            "src": "Modified Files/app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/Modified Files/home.html"
        },
        {
            "src": "/aboutus",
            "dest": "/Modified Files/aboutus.html"
        },
        {
            "src": "/diagonalize",
            "dest": "/Modified Files/diagonalize.html"
        },
        {
            "src": "/scripts/(.*)",
            "dest": "/Modified Files/scripts/$1"
        },
        {
            "src": "/style/(.*)",
            "dest": "/Modified Files/style/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "Modified Files/app.py"
        }
    ]
}
